#!/usr/bin/env bash

usage()
{
    cat <<EOF
Usage: kea-config [OPTIONS]
Options:
     [--prefix]
     [--version]
     [--libs]
     [--cflags]
     [--ldflags]
     [--includes]
     [--hdfincludes]
     [--hdfcflags]
     [--hdflibs]
EOF
    exit $1
}
if test $# -eq 0; then
  usage 1 1>&2
fi
while test $# -gt 0; do
case "$1" in
    -*=*) optarg=`echo "$1" | sed 's/[-_a-zA-Z0-9]*=//'` ;;
    *) optarg= ;;
esac
case $1 in
    --prefix)
    echo -n "/home/cayang/anaconda3/envs/Evaluate "
     ;;
	--version)
    echo -n "1.4.13 "
     ;;
    --cflags)
    echo -n "-I/home/cayang/anaconda3/envs/Evaluate/include "
      ;;
    --libs)
      echo -n "-L/home/cayang/anaconda3/envs/Evaluate/lib -lkea "
      ;;
    --ldflags)
      echo -n "/home/cayang/anaconda3/envs/Evaluate/lib "
      ;;
    --includes)
      echo -n "/home/cayang/anaconda3/envs/Evaluate/include "
      ;;
	--hdfincludes)
      echo -n " /home/cayang/anaconda3/envs/Evaluate/include "
      ;;
	--hdfcflags)
      echo -n " -I/home/cayang/anaconda3/envs/Evaluate/include "
      ;;
	--hdflibs)
      echo -n " -L/home/cayang/anaconda3/envs/Evaluate/lib -L/home/conda/feedstock_root/build_artifacts/kealib_1587158266507/_build_env/x86_64-conda_cos6-linux-gnu/sysroot/usr/lib -lhdf5_cpp -lhdf5 -lrt -lpthread -lz -ldl -lm "
      ;;
  esac
  shift
done
echo
